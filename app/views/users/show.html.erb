<div class='container '>
    <h2>User info</h2>
    <%= render 'info', user: @user %>
</div>
<div class='container '>
  <div class='row-md-8 offset-md-1'>
    <h2>投稿一覧</h2>
    <%= render 'run_index',runs: @runs %>
