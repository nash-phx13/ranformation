<div class='container px-5 px-sm-0'>
  <%= render 'layouts/errors', obj: @run %>
  <div class='row'>
    <div class='col-md-3'>
      <h2>タグリスト</h2>
       <%= render 'tags/index', tags: @tag_list %>
      <h2 class="mt-3">New Run</h2>
      <%= render 'form', run: @run %>
    </div>
    <div class='col-md-8 offset-md-1'>
      <h2>ランニング記事一覧</h2>
      <%= render 'index', runs: @runs%>
    </div>
  </div>
</div>